#include "stm32f4xx.h"
#include "FreeRTOS.h"
#include "Task.h"
			
TaskFunction_t hTask1;
TaskFunction_t hTask2;

int main(void)
{
	// 1. Deinitialize SystemCoreClock settings (default) of FreeRTOS to make the sysclock run at 16MHz
	RCC_DeInit();

	SystemCoreClockUpdate();

	// Create Task - 1


	for(;;);
}
